class Solution:
    def maxProduct(self, words: List[str]) -> int:
        max_res = 0
        for i in range(len(words)):
            for j in range(i+1,len(words)):
                if not (set(words[i]).intersection(set(words[j]))):
                    x = len(words[i]) * len(words[j])
                    max_res = x if x>max_res else max_res
                
        return max_res
